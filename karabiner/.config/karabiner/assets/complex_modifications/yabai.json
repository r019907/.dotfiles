{
  "title": "Map Ctrl+Option+Key to Fn+Key",
  "rules": [
    {
      "description": "Ctrl+Option+Key to Fn+Key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "any",
            "modifiers": {
              "mandatory": ["right_control", "left_option"]
            }
          },
          "to": [
            {
              "key_code": "any",
              "modifiers": ["fn"]
            }
          ]
        }
      ]
    }
  ]
}
