{
  "title": "Map Ctrl+Option+Key to Fn+Key",
  "rules": [
    {
      "description": "Ctrl+Option+Key to Fn+Key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "any": "key_code",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["left_option", "right_option"]
            }
          },
          "to": [
            {
              "key_code": "fn"
            }
          ]
        }
      ]
    }
  ]
}
