{
  "title": "Theme Switch",
  "rules": [
    {
      "description": "fn --> Theme Switching",
      "manipulators": [
        {
          "from": {
            "key_code": "f6"
          },
          "to": [
            {
              "shell_command": "open -a 'Safari.app'"
            }
          ]
        }
      ]
    }
  ]
}
